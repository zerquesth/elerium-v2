windowsFrame.Name = "Windows"
windowsFrame.Parent = screenGui
windowsFrame.BackgroundColor3 = Color3.new(1, 1, 1)
windowsFrame.BackgroundTransparency = 1
windowsFrame.Position = UDim2.new(0, 20, 0, 20)
windowsFrame.Size = UDim2.new(1, 20, 1, -20)

script.Parent = screenGui

local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local prefabs = script.Parent:WaitForChild("Prefabs")
local windows = script.Parent:FindFirstChild("Windows")

local state = {
   ["binding"] = false
}

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
   if input.KeyCode == config.toggle_key and not state.binding then
      screenGui.Enabled = not screenGui.Enabled
   end
end)

local function tweenObject(object, properties, duration)
   if not duration then
      duration = 0.5
   end
   local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
   local tween = TweenService:Create(object, tweenInfo, properties)
   tween:Play()
end

local function rgbToHsv(r, g, b)
   r, g, b = r / 255, g / 255, b / 255
   local max, min = math.max(r, g, b), math.min(r, g, b)
   local h, s, v = nil, nil, nil
   v = max
   local diff = max - min
   if max == 0 then
      s = 0
   else
      s = diff / max
   end
   if max == min then
      h = 0
   else
      if r == max then
         h = (g - b) / diff + (g < b and 6 or 0)
      elseif g == max then
         h = (b - r) / diff + 2
      else
         h = (r - g) / diff + 4
      end
      h = h / 6
   end
   return h, s, v
end

local function getMousePos()
   return Vector2.new(UserInputService:GetMouseLocation().X + 1, UserInputService:GetMouseLocation().Y - 35)
end

local function createClickEffect(targetFrame, x, y)
   spawn(function()
      targetFrame.ClipsDescendants = true
      local circle = prefabs:FindFirstChild("Circle"):Clone()
      circle.Parent = targetFrame
      circle.ZIndex = 1000
      local relativeX = x - circle.AbsolutePosition.X
      local relativeY = y - circle.AbsolutePosition.Y
      circle.Position = UDim2.new(0, relativeX, 0, relativeY)
      local size
      if targetFrame.AbsoluteSize.X > targetFrame.AbsoluteSize.Y then
         size = targetFrame.AbsoluteSize.X * 1.5
      elseif targetFrame.AbsoluteSize.X < targetFrame.AbsoluteSize.Y then
         size = targetFrame.AbsoluteSize.Y * 1.5
      else
         size = targetFrame.AbsoluteSize.X * 1.5
      end
      circle:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, -size / 2, 0.5, -size / 2), "Out", "Quad", 0.5, false, nil)
      tweenObject(circle, { ImageTransparency = 1 }, 0.5)
      wait(0.5)
      circle:Destroy()
   end)
end

local objectCounter = 0
local globalObjects = {}

local function formatWindows()
   for _, obj in next, windows:GetChildren() do
      if not obj:IsA("UIListLayout") then
         globalObjects[obj] = obj.AbsolutePosition
      end
   end
   for _, obj in next, globalObjects do
      obj.Position = UDim2.new(0, obj.X, 0, obj.Y)
   end
end

globalObjects["FormatWindows"] = formatWindows

globalObjects["AddWindow"] = function(windowConfig, windowTitle, windowConfigArgs)
   objectCounter = objectCounter + 1
   local isResizing = false
   windowTitle = tostring(windowTitle or "New Window")
   if typeof(windowConfigArgs) ~= "table" or not windowConfigArgs then
      windowConfigArgs = config
   end
   windowConfigArgs.tween_time = 0.1
   local window = prefabs:FindFirstChild("Window"):Clone()
   window.Parent = windows
   window:FindFirstChild("Title").Text = windowTitle
   window.Size = UDim2.new(0, windowConfigArgs.min_size.X, 0, windowConfigArgs.min_size.Y)
   window.ZIndex = window.ZIndex + objectCounter * 10
   do
      local title = window:FindFirstChild("Title")
      local bar = window:FindFirstChild("Bar")
      local barBase = bar:FindFirstChild("Base")
      local barTop = bar:FindFirstChild("Top")
      local tabSelectionFrame = window:FindFirstChild("TabSelection"):FindFirstChild("Frame")
      local toggle = bar:FindFirstChild("Toggle")
      spawn(function()
         while true do
            bar.BackgroundColor3 = windowConfigArgs.main_color
            barBase.BackgroundColor3 = windowConfigArgs.main_color
            barBase.ImageColor3 = windowConfigArgs.main_color
            barTop.ImageColor3 = windowConfigArgs.main_color
            tabSelectionFrame.BackgroundColor3 = windowConfigArgs.main_color
            RunService.Heartbeat:Wait()
         end
      end)
   end
   local resizer = window:WaitForChild("Resizer")
   local windowState = {}
   window.Draggable = true
   do
      local originalIcon = Mouse.Icon
      local isResizingEnabled = false
      resizer.MouseEnter:Connect(function()
         window.Draggable = false
         if windowConfigArgs.can_resize then
            originalIcon = Mouse.Icon
         end
         isResizingEnabled = true
      end)
      resizer.MouseLeave:Connect(function()
         isResizingEnabled = false
         if windowConfigArgs.can_resize then
            Mouse.Icon = originalIcon
         end
         window.Draggable = true
      end)
      local isMouseDown = false
      UserInputService.InputBegan:Connect(function(input)
         if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isMouseDown = true
            spawn(function()
               if isResizingEnabled and resizer.Active and windowConfigArgs.can_resize then
                  while isMouseDown and resizer.Active do
                     local mousePos = getMousePos()
                     local relativeX = mousePos.X - window.AbsolutePosition.X
                     local relativeY = mousePos.Y - window.AbsolutePosition.Y
                     if relativeX <= windowConfigArgs.min_size.X and relativeY <= windowConfigArgs.min_size.Y then
                        tweenObject(window, { Size = UDim2.new(0, relativeX, 0, relativeY) }, windowConfigArgs.tween_time)
                     else
                        if relativeX <= windowConfigArgs.min_size.X then
                           tweenObject(window, { Size = UDim2.new(0, relativeX, 0, windowConfigArgs.min_size.Y) }, windowConfigArgs.tween_time)
                        elseif relativeY <= windowConfigArgs.min_size.Y then
                           tweenObject(window, { Size = UDim2.new(0, windowConfigArgs.min_size.X, 0, relativeY) }, windowConfigArgs.tween_time)
                        else
                           tweenObject(window, { Size = UDim2.new(0, windowConfigArgs.min_size.X, 0, windowConfigArgs.min_size.Y) }, windowConfigArgs.tween_time)
                        end
                     end
                     RunService.Heartbeat:Wait()
                  end
               end
            end)
         end
      end)
      UserInputService.InputEnded:Connect(function(input)
         if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isMouseDown = false
         end
      end)
   end
   do
      local toggle = window:FindFirstChild("Bar"):FindFirstChild("Toggle")
      local isExpanded = true
      local hiddenTabs = {}
      local isToggling = true
      toggle.MouseButton1Click:Connect(function()
         if isToggling then
            isToggling = false
            if isExpanded then
               hiddenTabs = {}
               for _, tab in next, window:FindFirstChild("Tabs"):GetChildren() do
                  if not tab:IsA("UIListLayout") then
                     hiddenTabs[tab] = tab.Visible
                     tab.Visible = false
                  end
               end
               resizer.Active = false
               local currentHeight = window.AbsoluteSize.Y
               tweenObject(toggle, { Rotation = 0 }, windowConfigArgs.tween_time)
               tweenObject(window, { Size = UDim2.new(0, window.AbsoluteSize.X, 0, 26) }, windowConfigArgs.tween_time)
               toggle.Parent:FindFirstChild("Base").Transparency = 1
            else
               for tab, visibility in next, hiddenTabs do
                  tab.Visible = visibility
               end
               resizer.Active = true
               tweenObject(toggle, { Rotation = 90 }, windowConfigArgs.tween_time)
               tweenObject(window, { Size = UDim2.new(0, window.AbsoluteSize.X, 0, currentHeight) }, windowConfigArgs.tween_time)
               toggle.Parent:FindFirstChild("Base").Transparency = 0
            end
            isExpanded = not isExpanded
            wait(windowConfigArgs.tween_time)
            isToggling = true
         end
      end)
   end
   do
      local tabs = window:FindFirstChild("Tabs")
      local tabSelection = window:FindFirstChild("TabSelection")
      local tabButtons = tabSelection:FindFirstChild("TabButtons")
      local tabManager = {}
      tabManager.AddTab = function(tabConfig, tabTitle)
         tabTitle = tostring(tabTitle or "New Tab")
         tabSelection.Visible = true
         local tabButton = prefabs:FindFirstChild("TabButton"):Clone()
         tabButton.Parent = tabButtons
         tabButton.Text = tabTitle
         tabButton.Size = UDim2.new(0, tabButton.TextBounds.X + 15, 0, 20)
         tabButton.ZIndex = tabButton.ZIndex + objectCounter * 10
         tabButton:GetChildren()[1].ZIndex = tabButton:GetChildren()[1].ZIndex + objectCounter * 10
         local tab = prefabs:FindFirstChild("Tab"):Clone()
         tab.Parent = tabs
         tab.ZIndex = tab.ZIndex + objectCounter * 10
         local currentlySelected = false
         local function selectTab()
            if currentlySelected then return end
            for _, otherTabButton in next, tabButtons:GetChildren() do
               if not otherTabButton:IsA("UIListLayout") then
                  otherTabButton:GetChildren()[1].ImageColor3 = Color3.fromRGB(52, 53, 56)
                  tweenObject(otherTabButton, { Size = UDim2.new(0, otherTabButton.AbsoluteSize.X, 0, 20) }, windowConfigArgs.tween_time)
               end
            end
            for _, otherTab in next, tabs:GetChildren() do
               otherTab.Visible = false
            end
            tweenObject(tabButton, { Size = UDim2.new(0, tabButton.AbsoluteSize.X, 0, 25) }, windowConfigArgs.tween_time)
            tabButton:GetChildren()[1].ImageColor3 = Color3.fromRGB(73, 75, 79)
            tab.Visible = true
         end
         tabButton.MouseButton1Click:Connect(function()
            selectTab()
         end)
         local tabInterface = {}
         tabInterface.Show = function(optionalArg)
            selectTab()
         end
         tabInterface.AddLabel = function(labelConfig, labelText)
            labelText = tostring(labelText or "New Label")
            local label = prefabs:FindFirstChild("Label"):Clone()
            label.Parent = tab
            label.Text = labelText
            label.Size = UDim2.new(0, label.TextBounds.X + 15, 0, 20)
            label.ZIndex = label.ZIndex + objectCounter * 10
         end
         tabInterface.AddButton = function(buttonConfig, buttonText, onClick)
            buttonText = tostring(buttonText or "New Button")
            if typeof(onClick) ~= "function" or not onClick then
               onClick = function() end
            end
            local button = prefabs:FindFirstChild("Button"):Clone()
            button.Parent = tab
            button.Text = buttonText
            button.Size = UDim2.new(0, button.TextBounds.X + 15, 0, 20)
            button.ZIndex = button.ZIndex + objectCounter * 10
            button:GetChildren()[1].ZIndex = button:GetChildren()[1].ZIndex + objectCounter * 10
            spawn(function()
               while true do
                  if button and button:GetChildren()[1] then
                     button:GetChildren()[1].ImageColor3 = windowConfigArgs.main_color
                  end
                  RunService.Heartbeat:Wait()
               end
            end)
            button.MouseButton1Click:Connect(function()
               createClickEffect(button, Mouse.X, Mouse.Y)
               pcall(onClick)
            end)
         end
         tabInterface.AddSwitch = function(switchConfig, switchText, onChanged)
            local switchState = {}
            switchText = tostring(switchText or "New Switch")
            if typeof(onChanged) ~= "function" or not onChanged then
               onChanged = function() end
            end
            local switch = prefabs:FindFirstChild("Switch"):Clone()
            switch.Parent = tab
            switch:FindFirstChild("Title").Text = switchText
            switch:FindFirstChild("Title").ZIndex = switch:FindFirstChild("Title").ZIndex + objectCounter * 10
            switch.ZIndex = switch.ZIndex + objectCounter * 10
            switch:GetChildren()[1].ZIndex = switch:GetChildren()[1].ZIndex + objectCounter * 10
            spawn(function()
               while true do
                  if switch and switch:GetChildren()[1] then
                     switch:GetChildren()[1].ImageColor3 = windowConfigArgs.main_color
                  end
                  RunService.Heartbeat:Wait()
               end
            end)
            local isSwitchedOn = false
            switch.MouseButton1Click:Connect(function()
               isSwitchedOn = not isSwitchedOn
               switch.Text = isSwitchedOn and utf8.char(10003) or ""
               pcall(onChanged, isSwitchedOn)
            end)
            switch.Set = function(value)
               isSwitchedOn = typeof(value) == "boolean" and value or false
               switch.Text = isSwitchedOn and utf8.char(10003) or ""
               pcall(onChanged, isSwitchedOn)
            end
            local switchButton = switch
            local switchControl = switch
         end
         tabInterface.AddTextBox = function(textBoxConfig, placeholderText, onTextChanged, options)
            local textBoxState = {}
            placeholderText = tostring(placeholderText or "New TextBox")
            if typeof(onTextChanged) ~= "function" or not onTextChanged then
               onTextChanged = function() end
            end
            if typeof(options) ~= "table" or not options then
               options = { clear = true }
            end
            options = {
               clear = options.clear == true or false
            }
            local textBox = prefabs:FindFirstChild("TextBox"):Clone()
            textBox.Parent = tab
            textBox.PlaceholderText = placeholderText
            textBox.ZIndex = textBox.ZIndex + objectCounter * 10
            textBox:GetChildren()[1].ZIndex = textBox:GetChildren()[1].ZIndex + objectCounter * 10
            textBox.FocusLost:Connect(function(enterPressed)
               if enterPressed and 0 < #textBox.Text then
                  pcall(onTextChanged, textBox.Text)
                  if options.clear then
                     textBox.Text = ""
                  end
               end
            end)
         end
         tabInterface.AddSlider = function(sliderConfig, sliderText, onValueChanged, sliderOptions)
            local sliderState = {}
            sliderText = tostring(sliderText or "New Slider")
            if typeof(onValueChanged) ~= "function" or not onValueChanged then
               onValueChanged = function() end
            end
            if typeof(sliderOptions) ~= "table" or not sliderOptions then
               sliderOptions = {}
            end
            sliderOptions = {
               min = sliderOptions.min or 0,
               max = sliderOptions.max or 100,
               readonly = sliderOptions.readonly or false
            }
            local slider = prefabs:FindFirstChild("Slider"):Clone()
            slider.Parent = tab
            slider.ZIndex = slider.ZIndex + objectCounter * 10
            local sliderTitle = slider:FindFirstChild("Title")
            local sliderIndicator = slider:FindFirstChild("Indicator")
            local sliderValue = slider:FindFirstChild("Value")
            sliderTitle.ZIndex = sliderTitle.ZIndex + objectCounter * 10
            sliderIndicator.ZIndex = sliderIndicator.ZIndex + objectCounter * 10
            sliderValue.ZIndex = sliderValue.ZIndex + objectCounter * 10
            sliderTitle.Text = sliderText
            do
               local isHovering = false
               local isDragging = false
               slider.MouseEnter:Connect(function()
                  window.Draggable = false
                  isHovering = true
               end)
               slider.MouseLeave:Connect(function()
                  window.Draggable = true
                  isHovering = false
               end)
               local isMouseDown = false
               UserInputService.InputBegan:Connect(function(input)
                  if input.UserInputType == Enum.UserInputType.MouseButton1 then
                     isMouseDown = true
                     spawn(function()
                        if isHovering and not sliderOptions.readonly then
                           while isMouseDown and not currentlySelected do
                              local mousePos = getMousePos()
                              local relativeX = slider.AbsoluteSize.X - (mousePos.X - slider.AbsolutePosition.X) + 1 / slider.AbsoluteSize.X
                              local value = 0
                              local minValue = 0
                              local maxValue = 1
                              local normalizedValue = math.floor(relativeX / 100 * 100)
                              local finalValue = math.floor((maxValue / 100 * normalizedValue) + minValue)
                              tweenObject(sliderIndicator, { Size = UDim2.new(normalizedValue / 100 or minValue, 0, 0, 20) }, windowConfigArgs.tween_time)
                              local percentage = math.floor((normalizedValue / 100) * 100)
                              local max = sliderOptions.max
                              local min = sliderOptions.min
                              local range = max - min
                              local finalOutput = math.floor((range / 100) * percentage + min)
                              sliderValue.Text = tostring(finalOutput)
                              pcall(onValueChanged, finalOutput)
                              RunService.Heartbeat:Wait()
                           end
                        end
                     end)
                  end
               end)
               UserInputService.InputEnded:Connect(function(input)
                  if input.UserInputType == Enum.UserInputType.MouseButton1 then
                     isMouseDown = false
                  end
               end)
               slider.Set = function(value)
                  local colorValue = typeof(value) == "Color3" and value or Color3.new(1, 1, 1)
                  local h, s, v = rgbToHsv(colorValue.r * 255, colorValue.g * 255, colorValue.b * 255)
                  sliderIndicator.ImageColor3 = colorValue
                  slider.Saturation.ImageColor3 = Color3.fromHSV(h, 1, 1)
                  pcall(onValueChanged, colorValue)
               end
            end
            local sliderData = sliderConfig
            local sliderControl = slider
         end
         tabInterface.AddKeybind = function(keybindConfig, keybindText, onKeyDown, keybindOptions)
            local keybindState = {}
            keybindText = tostring(keybindText or "New Keybind")
            if typeof(onKeyDown) ~= "function" or not onKeyDown then
               onKeyDown = function() end
            end
            if typeof(keybindOptions) ~= "table" or not keybindOptions then
               keybindOptions = {}
            end
            keybindOptions = {
               standard = keybindOptions.standard or Enum.KeyCode.RightShift
            }
            local keybind = prefabs:FindFirstChild("Keybind"):Clone()
            local inputButton = keybind:FindFirstChild("Input")
            local titleLabel = keybind:FindFirstChild("Title")
            keybind.ZIndex = keybind.ZIndex + objectCounter * 10
            inputButton.ZIndex = inputButton.ZIndex + objectCounter * 10
            inputButton:GetChildren()[1].ZIndex = inputButton:GetChildren()[1].ZIndex + objectCounter * 10
            titleLabel.ZIndex = titleLabel.ZIndex + objectCounter * 10
            keybind.Parent = tab
            titleLabel.Text = "  "..keybindText
            keybind.Size = UDim2.new(0, titleLabel.TextBounds.X + 80, 0, 20)
            local keybindMap = {
               ["RightControl"] = "RightCtrl",
               ["LeftControl"] = "LeftCtrl",
               ["LeftShift"] = "LShift",
               ["RightShift"] = "RShift",
               ["MouseButton1"] = "Mouse1",
               ["MouseButton2"] = "Mouse2"
            }
            local currentKey = keybindOptions.standard
            keybindState.SetKeybind = function(newKey)
               local keyName = keybindMap[newKey.Name] or newKey.Name
               inputButton.Text = keyName
               currentKey = newKey
            end
            UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
               if state.binding then
                  spawn(function()
                     wait()
                     state.binding = false
                  end)
                  return
               end
               if input.KeyCode == currentKey and not gameProcessedEvent then
                  pcall(onKeyDown, currentKey)
               end
            end)
            keybindState:SetKeybind(keybindOptions.standard)
            inputButton.MouseButton1Click:Connect(function()
               if state.binding then return end
               inputButton.Text = "..."
               state.binding = true
               local input, gameProcessed = UserInputService.InputBegan:Wait()
               keybindState:SetKeybind(input.KeyCode)
            end)
            local keybindInterface = keybindState
            local currentKeyCode = currentKey
         end
         tabInterface.AddDropdown = function(dropdownConfig, dropdownText, onSelected)
            local dropdownState = {}
            dropdownText = tostring(dropdownText or "New Dropdown")
            if typeof(onSelected) ~= "function" or not onSelected then
               onSelected = function() end
            end
            local dropdown = prefabs:FindFirstChild("Dropdown"):Clone()
            local dropdownBox = dropdown:FindFirstChild("Box")
            local dropdownObjects = dropdownBox:FindFirstChild("Objects")
            local dropdownIndicator = dropdown:FindFirstChild("Indicator")
            dropdown.ZIndex = dropdown.ZIndex + objectCounter * 10
            dropdownBox.ZIndex = dropdownBox.ZIndex + objectCounter * 10
            dropdownObjects.ZIndex = dropdownObjects.ZIndex + objectCounter * 10
            dropdownIndicator.ZIndex = dropdownIndicator.ZIndex + objectCounter * 10
            dropdown:GetChildren()[3].ZIndex = dropdown:GetChildren()[3].ZIndex + objectCounter * 10
            dropdown.Parent = tab
            dropdown.Text = "      "..dropdownText
            dropdownBox.Size = UDim2.new(1, 0, 0, 0)
            local isExpanded = false
            dropdown.MouseButton1Click:Connect(function()
               isExpanded = not isExpanded
               local objectCount = #dropdownObjects:GetChildren() - 1 * 20
               if 10 <= #dropdownObjects:GetChildren() - 1 then
                  objectCount = 200
                  dropdownObjects.CanvasSize = UDim2.new(0, 0, (#dropdownObjects:GetChildren() - 1) * 0.1, 0)
               end
               if isExpanded then
                  if currentlySelected then return end
                  currentlySelected = true
                  tweenObject(dropdownBox, { Size = UDim2.new(1, 0, 0, objectCount) }, windowConfigArgs.tween_time)
                  tweenObject(dropdownIndicator, { Rotation = 90 }, windowConfigArgs.tween_time)
               else
                  currentlySelected = false
                  tweenObject(dropdownBox, { Size = UDim2.new(1, 0, 0, 0) }, windowConfigArgs.tween_time)
                  tweenObject(dropdownIndicator, { Rotation = -90 }, windowConfigArgs.tween_time)
               end
            end)
            dropdownState.Add = function(optionText, onSelect)
               local optionState = {}
               optionText = tostring(optionText or "New Object")
               local optionButton = prefabs:FindFirstChild("DropdownButton"):Clone()
               optionButton.Parent = dropdownObjects
               optionButton.Text = optionText
               optionButton.ZIndex = optionButton.ZIndex + objectCounter * 10
               optionButton.MouseEnter:Connect(function()
                  optionButton.BackgroundColor3 = windowConfigArgs.main_color
               end)
               optionButton.MouseLeave:Connect(function()
                  optionButton.BackgroundColor3 = Color3.fromRGB(33, 34, 36)
               end)
               if isExpanded then
                  local currentHeight = #dropdownObjects:GetChildren() - 1 * 20
                  if 10 <= #dropdownObjects:GetChildren() - 1 then
                     currentHeight = 200
                     dropdownObjects.CanvasSize = UDim2.new(0, 0, (#dropdownObjects:GetChildren() - 1) * 0.1, 0)
                  end
                  tweenObject(dropdownBox, { Size = UDim2.new(1, 0, 0, currentHeight) }, windowConfigArgs.tween_time)
               end
               optionButton.MouseButton1Click:Connect(function()
                  if currentlySelected then
                     dropdown.Text = "      [ "..optionText.." ]"
                     currentlySelected = false
                     isExpanded = false
                     tweenObject(dropdownBox, { Size = UDim2.new(1, 0, 0, 0) }, windowConfigArgs.tween_time)
                     tweenObject(dropdownIndicator, { Rotation = -90 }, windowConfigArgs.tween_time)
                     pcall(onSelected, optionText)
                  end
               end)
               optionButton.Remove = function()
                  optionButton:Destroy()
               end
               local optionControl = optionButton
               local optionInterface = optionState
            end
            local dropdownInterface = dropdownState
            local dropdownControl = dropdown
         end
         tabInterface.AddColorPicker = function(colorPickerConfig, onColorChanged)
            local colorPickerState = {}
            if typeof(onColorChanged) ~= "function" or not onColorChanged then
               onColorChanged = function() end
            end
            local colorPicker = prefabs:FindFirstChild("ColorPicker"):Clone()
            colorPicker.Parent = tab
            colorPicker.ZIndex = colorPicker.ZIndex + objectCounter * 10
            local palette = colorPicker:FindFirstChild("Palette")
            local sample = colorPicker:FindFirstChild("Sample")
            local saturation = colorPicker:FindFirstChild("Saturation")
            palette.ZIndex = palette.ZIndex + objectCounter * 10
            sample.ZIndex = sample.ZIndex + objectCounter * 10
            saturation.ZIndex = saturation.ZIndex + objectCounter * 10
            do
               local hue = 0
               local saturationVal = 1
               local value = 1
               local function updateColor()
                  local currentColor = Color3.fromHSV(hue, saturationVal, value)
                  sample.ImageColor3 = currentColor
                  saturation.ImageColor3 = Color3.fromHSV(hue, 1, 1)
                  pcall(onColorChanged, currentColor)
               end
               do
                  local initialColor = Color3.fromHSV(hue, saturationVal, value)
                  sample.ImageColor3 = initialColor
                  saturation.ImageColor3 = Color3.fromHSV(hue, 1, 1)
               end
               local isPaletteHovering = false
               local isSaturationHovering = false
               palette.MouseEnter:Connect(function()
                  window.Draggable = false
                  isPaletteHovering = true
               end)
               palette.MouseLeave:Connect(function()
                  window.Draggable = true
                  isPaletteHovering = false
               end)
               saturation.MouseEnter:Connect(function()
                  window.Draggable = false
                  isSaturationHovering = true
               end)
               saturation.MouseLeave:Connect(function()
                  window.Draggable = true
                  isSaturationHovering = false
               end)
               local paletteIndicator = palette:FindFirstChild("Indicator")
               local saturationIndicator = saturation:FindFirstChild("Indicator")
               paletteIndicator.ZIndex = paletteIndicator.ZIndex + objectCounter * 10
               saturationIndicator.ZIndex = saturationIndicator.ZIndex + objectCounter * 10
               local isMouseDown = false
               UserInputService.InputBegan:Connect(function(input)
                  if input.UserInputType == Enum.UserInputType.MouseButton1 then
                     isMouseDown = true
                     spawn(function()
                        while isMouseDown and isPaletteHovering and not currentlySelected do
                           local mousePos = getMousePos()
                           local relativeX = palette.AbsoluteSize.X - (mousePos.X - palette.AbsolutePosition.X) + 1
                           local relativeY = palette.AbsoluteSize.Y - (mousePos.Y - palette.AbsolutePosition.Y) + 1.5
                           hue = relativeX / 100
                           saturationVal = relativeY / 100
                           tweenObject(paletteIndicator, { Position = UDim2.new(0, math.abs(relativeX - 100) - paletteIndicator.AbsoluteSize.X / 2, 0, math.abs(relativeY - 100) - paletteIndicator.AbsoluteSize.Y / 2) }, windowConfigArgs.tween_time)
                           updateColor()
                           RunService.Heartbeat:Wait()
                        end
                        while isMouseDown and isSaturationHovering and not currentlySelected do
                           local mousePos = getMousePos()
                           local relativeY = palette.AbsoluteSize.Y - (mousePos.Y - palette.AbsolutePosition.Y) + 1.5
                           value = relativeY / 100
                           tweenObject(saturationIndicator, { Position = UDim2.new(0, 0, 0, math.abs(relativeY - 100)) }, windowConfigArgs.tween_time)
                           updateColor()
                           RunService.Heartbeat:Wait()
                        end
                     end)
                  end
               end)
               UserInputService.InputEnded:Connect(function(input)
                  if input.UserInputType == Enum.UserInputType.MouseButton1 then
                     isMouseDown = false
                  end
               end)
               colorPickerState.Set = function(color)
                  if typeof(color) ~= "Color3" or not color then
                     color = Color3.new(1, 1, 1)
                  end
                  local h, s, v = rgbToHsv(color.r * 255, color.g * 255, color.b * 255)
                  sample.ImageColor3 = color
                  saturation.ImageColor3 = Color3.fromHSV(h, 1, 1)
                  pcall(onColorChanged, color)
               end
            end
            local colorPickerInterface = colorPickerState
            local colorPickerControl = colorPicker
         end
         tabInterface.AddConsole = function(consoleConfig, consoleOptions)
            local consoleState = {}
            if typeof(consoleOptions) ~= "table" or not consoleOptions then
               consoleOptions = { readonly = true, full = false }
            end
            consoleOptions = {
               y = tonumber(consoleOptions.y) or 200,
               source = consoleOptions.source or "Logs",
               readonly = consoleOptions.readonly == true or false,
               full = consoleOptions.full == true or false
            }
            local console = prefabs:FindFirstChild("Console"):Clone()
            console.Parent = tab
            console.ZIndex = console.ZIndex + objectCounter * 10
            console.Size = UDim2.new(1, 0, consoleOptions.full and 1 or 0, consoleOptions.y)
            local consoleFrame = console:GetChildren()[1]
            local sourceTextBox = consoleFrame:FindFirstChild("Source")
            local linesLabel = consoleFrame:FindFirstChild("Lines")
            sourceTextBox.ZIndex = sourceTextBox.ZIndex + objectCounter * 10
            linesLabel.ZIndex = linesLabel.ZIndex + objectCounter * 10
            sourceTextBox.TextEditable = not consoleOptions.readonly
            for _, child in next, sourceTextBox:GetChildren() do
               child.ZIndex = child.ZIndex + objectCounter * 10 + 1
            end
            sourceTextBox.Comments.ZIndex = sourceTextBox.Comments.ZIndex + 1
            do
               local keywords = {
                  "and", "break", "do", "else", "elseif", "end", "false", "for", "function", "goto", "if", "in", "local", "nil", "not", "or", "repeat", "return", "then", "true", "until", "while"
               }
               local globals = {
                  "getrawmetatable", "newcclosure", "islclosure", "setclipboard", "game", "workspace", "script", "math", "string", "table", "print", "wait", "BrickColor", "Color3", "next", "pairs", "ipairs", "select", "unpack", "Instance", "Vector2", "Vector3", "CFrame", "Ray", "UDim2", "Enum", "assert", "error", "warn", "tick", "loadstring", "_G", "shared", "getfenv", "setfenv", "newproxy", "setmetatable", "getmetatable", "os", "debug", "pcall", "ypcall", "xpcall", "rawequal", "rawset", "rawget", "tonumber", "tostring", "type", "typeof", "_VERSION", "coroutine", "delay", "require", "spawn", "LoadLibrary", "settings", "stats", "time", "UserSettings", "version", "Axes", "ColorSequence", "Faces", "ColorSequenceKeypoint", "NumberRange", "NumberSequence", "NumberSequenceKeypoint", "gcinfo", "elapsedTime", "collectgarbage", "PhysicalProperties", "Rect", "Region3", "Region3int16", "UDim", "Vector2int16", "Vector3int16", "load", "fire", "Fire"
               }
               local function highlight(text, highlightTable)
                  local highlighted = ""
                  local inputText = text
                  local replacements = {}
                  for key in pairs(highlightTable) do
                     replacements[key] = true
                  end
                  inputText = inputText:gsub(".", function(char)
                     if replacements[char] ~= nil then return " " end
                  end)
                  inputText = inputText:gsub("%S+", function(word)
                     if replacements[word] ~= nil then return word end
                  end)
               end
               local function formatNumbers(str)
                  local result = ""
                  str:gsub(".", function(char)
                     if tonumber(char) ~= nil then result = result..char else
                        if char == "" then result = result.."" else
                           if char == "	" then result = result.."	" else
                              result = result.." "
                           end
                        end
                     end
                  end)
               end
               local function formatStrings(str)
                  local result = ""
                  local inString = false
                  str:gsub(".", function(char)
                     if not inString and char == '"' then
                        inString = true
                     elseif inString and char == '"' then
                        inString = false
                     end
                     if inString and char == '"' then
                        result = result..'"'
                     else
                        if char == "" then
                           result = result..""
                        elseif char == "	" then
                           result = result.."	"
                        else
                           if inString == true then
                              result = result..char
                           else
                              if inString == false then
                                 result = result.." "
                              end
                           end
                        end
                     end
                  end)
               end
               local function formatTokens(str)
                  local result = ""
                  local inBracket = false
                  str:gsub(".", function(char)
                     if not inBracket and char == "[" then
                        inBracket = true
                     elseif inBracket and char == "]" then
                        inBracket = false
                     end
                     if inBracket and char == "]" then
                        result = result.."]"
                     else
                        if char == "" then
                           result = result..""
                        elseif char == "	" then
                           result = result.."	"
                        else
                           if inBracket == true then
                              result = result..char
                           else
                              if inBracket == false then
                                 result = result.." "
                              end
                           end
                        end
                     end
                  end)
               end
               local function formatComments(str)
                  local result = ""
                  local inComment = false
                  str:gsub("[^]+", function(line)
                     local isLineComment = false
                     local lineIndex = 0
                     line:gsub(".", function(char)
                        lineIndex = lineIndex + 1
                        if line:sub(lineIndex, lineIndex + 1) == "--" then
                           isLineComment = true
                        end
                        if isLineComment then
                           result = result..char
                        else
                           result = result.." "
                        end
                     end)
                     result = result
                  end)
               end
               local function highlightLua(text)
                  if text == "Text" then
                     sourceTextBox.Text = sourceTextBox.Text:gsub("", "")
                     sourceTextBox.Text = sourceTextBox.Text:gsub("	", "      ")
                     do
                        local inputText = sourceTextBox.Text
                        sourceTextBox.Keywords.Text = highlight(inputText, keywords)
                        sourceTextBox.Globals.Text = highlight(inputText, globals)
                        sourceTextBox.RemoteHighlight.Text = highlight(inputText, { "FireServer", "fireServer", "InvokeServer", "invokeServer" })
                        sourceTextBox.Tokens.Text = formatTokens(inputText)
                        sourceTextBox.Numbers.Text = formatNumbers(inputText)
                        sourceTextBox.Strings.Text = formatStrings(inputText)
                        sourceTextBox.Comments.Text = formatComments(inputText)
                        local lineCount = 1
                        inputText:gsub("", function()
                           lineCount = lineCount + 1
                        end)
                        linesLabel.Text = ""
                        for i = 1, lineCount, 1 do
                           linesLabel.Text = linesLabel.Text..i..""
                        end
                        sourceTextBox.CanvasSize = UDim2.new(0, 0, lineCount * 0.153846154, 0)
                     end
                  end
               end
               local function highlightLogs(text)
               end
               local function updateConsoleLines(text)
                  if text == "Text" then
                     sourceTextBox.Text = sourceTextBox.Text:gsub("", "")
                     sourceTextBox.Text = sourceTextBox.Text:gsub("	", "      ")
                     do
                        local inputText = sourceTextBox.Text
                        sourceTextBox.Info.Text = formatTokens(inputText)
                        local lineCount = 1
                        inputText:gsub("", function()
                           lineCount = lineCount + 1
                        end)
                        consoleScrollingFrame.CanvasSize = UDim2.new(0, 0, lineCount * 0.153846154, 0)
                     end
                  end
               end
               if consoleOptions.source == "Lua" then
                  highlightLua("Text")
                  sourceTextBox.Changed:Connect(highlightLua)
               elseif consoleOptions.source == "Logs" then
                  linesLabel.Visible = false
                  highlightLogs("Text")
                  sourceTextBox.Changed:Connect(highlightLogs)
               end
               consoleState.Set = function(text)
                  sourceTextBox.Text = tostring(text)
               end
               consoleState.Get = function()
                  local currentText = sourceTextBox.Text
                  return currentText
               end
               consoleState.Log = function(message)
                  sourceTextBox.Text = sourceTextBox.Text.."[*] "..tostring(message)..""
               end
            end
            local consoleInterface = consoleState
            local consoleControl = console
         end
         tabInterface.AddHorizontalAlignment = function(alignmentConfig)
            local alignmentState = {}
            local alignmentFrame = prefabs:FindFirstChild("HorizontalAlignment"):Clone()
            alignmentFrame.Parent = tab
            alignmentState.AddButton = function(buttonConfig, ...)
               local button, buttonControl
               local buttonResult = tabInterface.AddButton(...)
               if typeof(buttonResult[1]) == "table" then
                  button = buttonResult[1]
                  buttonControl = buttonResult[2]
                  buttonControl.Parent = alignmentFrame
                  return button, buttonControl
               end
               buttonControl = buttonResult[1]
               buttonControl.Parent = alignmentFrame
            end
            local alignmentInterface = alignmentState
            local alignmentControl = alignmentFrame
         end
         tabInterface.AddFolder = function(folderConfig, folderTitle)
            local folderState = {}
            folderTitle = tostring(folderTitle or "New Folder")
            local folder = prefabs:FindFirstChild("Folder"):Clone()
            local folderButton = folder:FindFirstChild("Button")
            local folderObjects = folder:FindFirstChild("Objects")
            local folderToggle = folderButton:FindFirstChild("Toggle")
            folder.ZIndex = folder.ZIndex + objectCounter * 10
            folderButton.ZIndex = folderButton.ZIndex + objectCounter * 10
            folderObjects.ZIndex = folderObjects.ZIndex + objectCounter * 10
            folderToggle.ZIndex = folderToggle.ZIndex + objectCounter * 10
            folderButton:GetChildren()[1].ZIndex = folderButton:GetChildren()[1].ZIndex + objectCounter * 10
            folder.Parent = tab
            folderButton.Text = "      "..folderTitle
            local function calculateFolderHeight()
               local height = 20
               for _, obj in next, folderObjects:GetChildren() do
                  if not obj:IsA("UIListLayout") then
                     height = height + obj.AbsoluteSize.Y + 5
                  end
               end
               return height
            end
            local isExpanded = false
            folderButton.MouseButton1Click:Connect(function()
               if isExpanded then
                  tweenObject(folderToggle, { Rotation = 0 }, windowConfigArgs.tween_time)
                  folderObjects.Visible = false
               else
                  tweenObject(folderToggle, { Rotation = 90 }, windowConfigArgs.tween_time)
                  folderObjects.Visible = true
               end
               isExpanded = not isExpanded
            end)
            spawn(function()
               while true do
                  folder.Size = UDim2.new(1, 0, 0, isExpanded and calculateFolderHeight() or 20)
                  wait()
               end
            end)
            for name, func in next, folderState do
               folder.Add = function(childName, ...)
                  local child, childControl
                  local childResult = tabInterface[name](...)
                  if typeof(childResult[1]) == "table" then
                     child = childResult[1]
                     childControl = childResult[2]
                     childControl.Parent = folderObjects
                     return child, childControl
                  end
                  childControl = childResult[1]
                  childControl.Parent = folderObjects
               end
            end
            local folderInterface = folderState
            local folderControl = folder
         end
         local tabInterface = tabManager
         local tabControl = tab
      end
   end
   local mainTabManager = tabManager
   local mainWindow = window
end
